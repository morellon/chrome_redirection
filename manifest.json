{
  "name": "Redirect to Download Pages",
  "version": "1.0",
  "description": "The first extension that I made.",
  "content_scripts": [
     {
       "matches": ["http://lix.in/*"],
       "css": [],
       "js": ["jquery.js", "lixin.js"]
     },
     {
       "matches": ["http://upmirror.com/*"],
       "css": [],
       "js": ["jquery.js", "upmirror.js"]
     }
   ],
  "browser_action": {
    "default_icon": "icon.png"
  }
}